                <ImageView
                    android:id="@+id/editDistrict"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginEnd="24dp"
                    android:src="@drawable/add_icon"
                    app:layout_constraintBottom_toBottomOf="@+id/userDistrict"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/userDistrict" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/district_view"
                style="@style/ICON_AND_STYLE"
                android:layout_width="match_parent"
                android:layout_height="4dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp" />

            <TextView
                android:id="@+id/city"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="52dp"
                android:fontFamily="@font/oswald_semi_bold"
                android:text="@string/profile_page_text8"
                android:textSize="25sp" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/userCity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginBottom="3dp"
                    android:fontFamily="@font/oswald_semi_bold"
                    android:text="TextView"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/editCity"
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginEnd="24dp"
                    android:src="@drawable/add_icon"
                    app:layout_constraintBottom_toBottomOf="@+id/userCity"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/userCity" />
            </androidx.constraintlayout.widget.ConstraintLayout>

